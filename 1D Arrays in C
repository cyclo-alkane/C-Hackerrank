//1D Arrays in C

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int i,j,n,sum=0;
   scanf("%d",&n);
   int a[n];
   for(i=0;i<n;i++)
    {
        scanf("%d ",&a[i]);
    }
    for(j=0;j<n;j++)
    {
        sum=sum+a[j];
    }
    printf("%d\n",sum);
    return 0;
}

